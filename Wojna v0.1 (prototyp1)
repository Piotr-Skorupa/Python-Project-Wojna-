#pakujemy karty do tali

talia = [dwapik, dwakaro, dwakier, dwatrefl, trzykaro, trzykier, trzypik, trzytrefl, czterykaro, czterykier, czterypik,
         czterytrefl, piectrefl, pieckaro, piecpik, pieckier, szesckaro, szesckier, szescpik, szesctrefl, siedemkaro,
         siedemkier, siedempik, siedemtrefl, osiemkaro, osiemkier, osiempik, osiemtrefl, dziewieckaro, dziewieckier,
         dziewiecpik, dziewiectrefl, dziesieckaro, dziesieckier, dziesiecpik, dziesiectrefl, waletkaro, waletkier,
         waletpik, walettrefl, damakaro, damakier, damapik, damatrefl, krolkaro, krolkier, krolpik, kroltrefl, askaro,
         askier, aspik, astrefl, joker, joker2]

#tworzymy funkcje tasujaca karty

def tasowanie(talia):
    random.shuffle(talia)
    return talia

#funkcja rozdajaca karty graczowi i komputerowi

def rozdawanie(talia):
    global gracz
    global komp
    gracz = talia[:27]
    komp = talia[27:]



tasing = tasowanie(talia)
rozdawing = rozdawanie(talia)
wynikgracza = 0
wynikkompa = 0
for znak in gracz:
    if gracz[znak.strange] > komp[znak.strange]:
        wynikgracza = wynikgracza + 1

    elif komp[znak.strange] > gracz[znak.strange]:
        wynikkompa = wynikkompa + 1

if wynikgracza > wynikkompa:
    print "Wygrales"
else:
    print "Przegrales"
